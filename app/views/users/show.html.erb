
<h2 class='header'><%= @user.fname %> <%= @user.lname %>'s Profile</h2>





<% @user.blogs.all.reverse[0..20].each do |b| %>

<div class='blog-container-home'>
<div class='post-container'>
	<div id='title-box'>
		<div class='post-title'><a href="/blogs/<%= b.id %>" onClick="window.location.reload()"><%= b.title%></a></div>
	</div>

	<div class='post-content'><%= raw b.content %></div>
</div>
</div>
<hr id='line-break'>

<% end %>
